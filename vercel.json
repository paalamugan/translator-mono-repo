{
    "version": 2,
    "env": {
        "NODE_ENV": "production"
    },
    "builds": [
        {
            "src": "server/src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "server/src/index.ts",
          "methods": ["GET", "POST", "PUT", "DELETE"]
        }
      ]
}